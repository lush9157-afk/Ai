{
  "name": "xeonix-deployment-ecosystem",
  "version": "1.0.0",
  "description": "Enterprise-grade VPS Management Ecosystem with Discord Bot, Web Dashboard, and Auto-Deployment",
  "main": "backend/src/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon backend/src/index.js",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node backend/src/index.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "test": "jest --coverage",
    "lint": "eslint backend/src frontend/src",
    "migrate": "node backend/src/database/migrate.js",
    "seed": "node backend/src/database/seed.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "setup": "npm install && cd frontend && npm install"
  },
  "keywords": [
    "vps",
    "discord-bot",
    "deployment",
    "docker",
    "lxc",
    "automation",
    "cloud-management"
  ],
  "author": "Xeonix Deployment Team",
  "license": "MIT",
